# get support
import pyre

# build a shell
shell = pyre.shells.interactive()(name='pyre.shell')
# make a context
context = {
    'pyre': pyre
}
# make a banner
banner = pyre.meta.header

# and launch it
shell.pyre_interactiveSession(banner=banner, context=context)

